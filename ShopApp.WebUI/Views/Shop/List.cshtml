@model ProductListModel

@{
    ViewData["Title"] = "Magic Shopping";
}

<div class="text-center">

    <div class="row">
        <div class="col-md-3">
            <ul class="list-group">
                
                   <li class="list-group-item">ELEKTRONİK</li>
                
            </ul>

        </div>
        <div class="col-md-9">

            <div class="row">
                @foreach (var product in Model.Products)
                {
                    <partial name="_product" model="@product" />
                }
            </div>
        </div>
    </div>




</div>
